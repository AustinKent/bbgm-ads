{
  "name": "bbgm-ads",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "prebid.js": "^2.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "gpt-mock": "^1.0.2",
    "husky": "^1.3.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^6.1.2",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4",
    "proclaim": "^3.6.0",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "scripts": {
    "build": "rm -rf dist && rm -rf temp && NODE_NO_WARNINGS=1 node scripts/build/index.js",
    "karma": "karma start",
    "lint": "eslint 'scripts/**/*.js' 'src/**/*.js' 'test/**/*.js'",
    "prettier": "prettier --write '*.js' 'scripts/**/*.js' 'src/**/*.js' 'test/**/*.js'",
    "test": "yarn run lint && yarn run karma"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
